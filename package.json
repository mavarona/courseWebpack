{
    "name": "webpack-starter-kit",
    "version": "1.0.0",
    "description": "course webpack",
    "main": "webpack.conf.js",
    "scripts": {
        "build": "webpack --mode production",
        "dev": "webpack --mode development",
        "build2": "webpack --mode production ./foo/src/index.js --output ./foo/main.js",
        "dev2": "webpack --mode development ./foo/src/index.js --output ./foo/main.js",
        "build-babel": "webpack --mode production --module-bind js=babel-loader",
        "dev-babel": "webpack --mode development --module-bind js=babel-loader",
        "start": "webpack-dev-server --mode development --open"
    },
    "author": "mavl",
    "license": "MIT",
    "devDependencies": {
        "autoprefixer": "^9.0.1",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^1.0.0",
        "file-loader": "^1.1.11",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.1",
        "node-sass": "^4.9.2",
        "postcss-loader": "^2.1.6",
        "resolve-url-loader": "^2.3.0",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "webpack": "^4.16.3",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.4"
    },
    "dependencies": {
        "image-webpack-loader": "^4.3.1"
    }
}